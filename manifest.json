{
  "name": "JMaps",
  "manifest_version": 2,
  "version":"0.0.1",
  "description": "JMaps authentication extension",
  "browser_action": {
    "name": "",
    "icons": ["facebook.png"],
    "default_icon": "facebook.png",
    "default_popup" : "popup.html"
  },
  "background": {
        "persistent":true,
        "page":"background.html"
  },
  "content_scripts": [ {
    "js": ["jquery-2.2.4.min.js"],
    "run_at": "document_end",
    "css":[],
    "matches": [ "http://www.google.co.in/*",
     "https://www.google.co.in/*"]
  }],
  "permissions": [
    "webRequest",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
